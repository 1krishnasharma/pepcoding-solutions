import java.util.*;
    
    public class Main{
    
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      long n=sc.nextLong();
      long[] a= new long[100];
      int i=0;
      while(n>0)
      {
          a[i]=n%10;
          n/=10;
          i++;
      }
      for(int j=i-1;j>=0;j--){
          System.out.print(a[j]);
          System.out.println()
;      } 
     }
    }
